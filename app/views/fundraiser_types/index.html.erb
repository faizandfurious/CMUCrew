<h1>Fundraiser Types</h1>
<table class="table table-bordered table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Created at</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @fundraiser_types.each do |fundraiser_type| %>
      <tr>
        <td><%= fundraiser_type.id %></td>
        <td><%= link_to fundraiser_type.name, fundraiser_type_path(fundraiser_type) %></td>
        <td><%= fundraiser_type.created_at %></td>
        <td>
          <%= link_to 'Edit', edit_fundraiser_type_path(fundraiser_type), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', fundraiser_type_path(fundraiser_type), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_fundraiser_type_path, :class => 'btn btn-primary' %>
